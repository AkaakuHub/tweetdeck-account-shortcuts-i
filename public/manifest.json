{
  "manifest_version": 2,
  "name": "TweetDeck Account Shortcuts(compatible with i/tweetdeck)",
  "version": "3.2.2",
  "description": "Add keyboard shortcuts to quickly select account to tweet from in TweetDeck",
  "icons": {
    "16": "icons/icon_16.png",
    "128": "icons/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://x.com/i/tweetdeck"
      ],
      "run_at": "document_end",
      "js": [
        "contentScript.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}